{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": ["warn", "single"],
    "func-names": ["error", "never"],
    "jsx-a11y/anchor-is-valid": ["off"],
    "jsx-a11y/no-noninteractive-element-interactions": [
      "off",
      {
        "handlers": [
          "onClick",
          "onMouseDown",
          "onMouseUp",
          "onKeyPress",
          "onKeyDown",
          "onKeyUp"
        ]
      }
    ]
  }
}
